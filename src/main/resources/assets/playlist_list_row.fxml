<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<AnchorPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox fx:id="uiSongsCategoryContainer" spacing="12.0">
         <children>
            <ImageView fx:id="uiIcon" fitHeight="22.0" fitWidth="22.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@/images/list.png" />
               </image>
            </ImageView>
            <Label fx:id="uiName" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Songs" textFill="#e1e1e1" HBox.hgrow="ALWAYS">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets bottom="4.0" left="12.0" right="12.0" top="4.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
